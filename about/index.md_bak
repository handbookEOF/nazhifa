    <script>
        // When the user scrolls down 50px from the top of the document, resize the header's font size
        window.onscroll = function() {
                var colXs = {
                        maxOfWidth:window.matchMedia("(max-width: 575px)"),
                        height:"0.5rem",
                        marginTop: "10px",
                        marginBottom: "0px",
                        heightTo:"1.5rem",
                        marginTopTo: "2rem",
                        marginBottomTo: "2rem"
                };
                var colSm = {
                        maxOfWidth:window.matchMedia("(max-width: 768px)"),
                        height:"0.5rem",
                        marginTop: "10px",
                        marginBottom: "0px",
                        heightTo:"1.5rem",
                        marginTopTo: "2rem",
                        marginBottomTo: "2rem"
                };
                var colMd = {
                        maxOfWidth:window.matchMedia("(max-width: 991px)"),
                        height:"0.8rem",
                        marginTop: "10px",
                        marginBottom: "0px",
                        heightTo:"4rem",
                        marginTopTo: "6rem",
                        marginBottomTo: "6rem"
                };
                var colLg = {
                        maxOfWidth:window.matchMedia("(max-width: 1199px)"),
                        height:"1rem",
                        marginTop: "10px",
                        marginBottom: "0px",
                        heightTo:"4rem",
                        marginTopTo: "6rem",
                        marginBottomTo: "6rem"
                };
                var colXl = {
                        maxOfWidth:window.matchMedia("(min-width: 1200px)"),
                        height:"1rem",
                        marginTop: "10px",
                        marginBottom: "0px",
                        heightTo:"4rem",
                        marginTopTo: "6rem",
                        marginBottomTo: "6rem"
                };
                if (colXs["maxOfWidth"].matches){
                        var m = colXs;
                        scrollFunction(m);
                } else if (colSm["maxOfWidth"].matches){
                        var m = colSm;
                        scrollFunction(m);
                } else if (colMd["maxOfWidth"].matches){
                        var m = colMd;
                        scrollFunction(m);
                } else if (colLg["maxOfWidth"].matches){
                        var m = colLg;
                        scrollFunction(m);
                } else {
                        var m = colXl;
                        scrollFunction(m);
                }

        };

        function scrollFunction(m) {
          if (document.body.scrollTop > 30 || document.documentElement.scrollTop > 30) {
                document.getElementById("nazhifaHeader").style.height = m.height;
                document.getElementById("nazhifaHeader").style.marginTop = m.marginTop;
                document.getElementById("nazhifaHeader").style.marginBottom = m.marginBottom;
          } else {
                document.getElementById("nazhifaHeader").style.height = m.heightTo;
                document.getElementById("nazhifaHeader").style.marginTop = m.marginTopTo;
                document.getElementById("nazhifaHeader").style.marginBottom = m.marginBottomTo;
          }
        }


    </script>
